{
  "name": "@jlnstack/i18n",
  "description": "Type-safe, tree-shakeable i18n with RSC support",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "https://github.com/jln13x/jlnstack"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "next": ">=15",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chokidar": "^4",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^26.0.0",
    "next": "^15.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "bin": {
    "i18n": "./dist/cli/index.mjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.mts",
        "default": "./dist/next.mjs"
      },
      "require": {
        "types": "./dist/next.d.cts",
        "default": "./dist/next.cjs"
      }
    }
  }
}
