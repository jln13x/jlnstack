{
  "name": "@jlnstack/schema",
  "description": "Schema versioning and migration utilities",
  "version": "0.0.1",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "https://github.com/jln13x/jlnstack"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "zod": ">=3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@standard-schema/spec": "^1.1.0",
    "@vitest/ui": "^2.1.8",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.3",
    "valibot": "^1.0.0",
    "vitest": "^2.1.8",
    "zod": "^3.24.1"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./zod": {
      "import": {
        "types": "./dist/zod/index.d.mts",
        "default": "./dist/zod/index.mjs"
      },
      "require": {
        "types": "./dist/zod/index.d.cts",
        "default": "./dist/zod/index.cjs"
      }
    }
  }
}
