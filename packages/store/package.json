{
  "name": "@jlnstack/store",
  "description": "Type-safe store",
  "version": "0.0.1",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "https://github.com/jln13x/jlnstack"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "react": ">=18",
    "zustand": ">=5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^2.1.8",
    "tsdown": "^0.15.4",
    "typescript": "catalog:",
    "vitest": "^2.1.8",
    "zustand": "^5.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./react": {
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    }
  }
}
