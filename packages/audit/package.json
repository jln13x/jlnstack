{
  "name": "@jlnstack/audit",
  "description": "Type-safe audit logging",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "https://github.com/jln13x/jlnstack"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "drizzle-orm": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.8",
    "drizzle-orm": "^0.38.3",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./drizzle": {
      "import": {
        "types": "./dist/drizzle.d.mts",
        "default": "./dist/drizzle.mjs"
      },
      "require": {
        "types": "./dist/drizzle.d.cts",
        "default": "./dist/drizzle.cjs"
      }
    },
    "./adapters/console": {
      "import": {
        "types": "./dist/adapters/console.d.mts",
        "default": "./dist/adapters/console.mjs"
      },
      "require": {
        "types": "./dist/adapters/console.d.cts",
        "default": "./dist/adapters/console.cjs"
      }
    },
    "./adapters/memory": {
      "import": {
        "types": "./dist/adapters/memory.d.mts",
        "default": "./dist/adapters/memory.mjs"
      },
      "require": {
        "types": "./dist/adapters/memory.d.cts",
        "default": "./dist/adapters/memory.cjs"
      }
    }
  }
}
