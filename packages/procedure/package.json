{
  "name": "@jlnstack/procedure",
  "description": "Type-safe procedure builder",
  "version": "0.0.4",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jln13x/jlnstack.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "next": ">=16"
  },
  "devDependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^2.1.8",
    "tsdown": "^0.15.4",
    "typescript": "catalog:",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.mts",
        "default": "./dist/next.mjs"
      },
      "require": {
        "types": "./dist/next.d.cts",
        "default": "./dist/next.cjs"
      }
    }
  }
}
