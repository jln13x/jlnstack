{
  "name": "@jlnstack/crypto",
  "description": "Type-safe cryptography utilities with JWT and encryption",
  "version": "0.0.1",
  "license": "MIT",
  "author": "jln13x",
  "repository": {
    "type": "git",
    "url": "https://github.com/jln13x/jlnstack"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@standard-schema/spec": "^1.1.0",
    "@vitest/ui": "^2.1.8",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./jwt": {
      "import": {
        "types": "./dist/jwt/index.d.mts",
        "default": "./dist/jwt/index.mjs"
      },
      "require": {
        "types": "./dist/jwt/index.d.cts",
        "default": "./dist/jwt/index.cjs"
      }
    },
    "./encrypt": {
      "import": {
        "types": "./dist/encrypt/index.d.mts",
        "default": "./dist/encrypt/index.mjs"
      },
      "require": {
        "types": "./dist/encrypt/index.d.cts",
        "default": "./dist/encrypt/index.cjs"
      }
    }
  }
}
